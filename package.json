{
	"name": "vscode-postfix-ts",
	"displayName": "TS/JS postfix completion",
	"description": "Postfix notation for TypeScript/Javascript",
	"version": "1.2.0",
	"license": "MIT",
	"publisher": "ipatalas",
	"preview": true,
	"engines": {
		"vscode": "^1.10.0"
	},
	"icon": "images/logo.png",
	"categories": [
		"Other"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/ipatalas/vscode-postfix-ts"
	},
	"activationEvents": [
		"onLanguage:typescript",
		"onLanguage:javascript",
		"onLanguage:tests"
	],
	"main": "./out/src/extension",
	"contributes": {
		"languages": [
			{
				"id": "tests"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "tsc -p ./",
		"compile": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/lodash": "^4.14.61",
		"@types/mocha": "^2.2.40",
		"@types/node": "^6.0.40",
		"decache": "^4.1.0",
		"istanbul": "^0.4.5",
		"mocha": "^3.2.0",
		"remap-istanbul": "^0.9.5",
		"tslint": "^5.0.0",
		"tslint-config-standard": "^4.0.0",
		"tslint-eslint-rules": "^3.5.1",
		"vscode": "^1.1.0"
	},
	"dependencies": {
		"glob": "^7.1.1",
		"lodash": "^4.17.4",
		"typescript": "^2.2.2"
	}
}
